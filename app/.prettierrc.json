{
    "printWidth": 105,
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "quoteProps": "preserve",
    "trailingComma": "none",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "importOrderSeparation": true,
    "importOrder": [
        "^@components/(.*)$",
        "^@pages/(.*)$",
        "^@store/(.*)$",
        "^@api/(.*)$",
        "^@utils/(.*)$",
        "^@model/(.*)$",
        "^@theme/(.*)$",
        "^[./]"
    ]
}
